include(CMakeFindDependencyMacro)

@PACKAGE_INIT@

set(@PROJECT_NAME@_PROJECT_DEPENDENCIES "@EXPORT_#PROJECT_NAME#_PROJECT_DEPENDENCIES@")
foreach(@PROJECT_NAME@_PROJECT_DEPENDENCY ${@PROJECT_NAME@_PROJECT_DEPENDENCIES})
	string(REPLACE " " ";" @PROJECT_NAME@_PROJECT_DEPENDENCY_LIST ${@PROJECT_NAME@_PROJECT_DEPENDENCY})
	if(CMAKE_VERSION LESS 3.9)
		list(REMOVE_ITEM @PROJECT_NAME@_PROJECT_DEPENDENCY_LIST "CONFIG")
	endif()
	find_dependency(${@PROJECT_NAME@_PROJECT_DEPENDENCY_LIST})
endforeach()
unset(@PROJECT_NAME@_PROJECT_DEPENDENCIES)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
check_required_components("@PROJECT_NAME@")
