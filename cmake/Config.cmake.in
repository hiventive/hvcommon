include(CMakeFindDependencyMacro)

@PACKAGE_INIT@

set(PROJECT_DEPENDENCIES "@PROJECT_DEPENDENCIES@")
foreach(PROJECT_DEPENDENCY ${PROJECT_DEPENDENCIES})
	string(REPLACE " " ";" PROJECT_DEPENDENCY_LIST ${PROJECT_DEPENDENCY})
	if(CMAKE_VERSION LESS 3.9)
		list(REMOVE_ITEM PROJECT_DEPENDENCY_LIST "CONFIG")
	endif()
	find_dependency(${PROJECT_DEPENDENCY_LIST})
endforeach()

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
check_required_components("@PROJECT_NAME@")
